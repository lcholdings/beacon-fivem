    "@types/node": "^22.15.32",
    "esbuild": "^0.25.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@beacon-oss/types": "^0.1.0-alpha.25",
    "@biomejs/biome": "^1.9.4",
    "@communityox/ox_lib": "latest",
    "@nativewrappers/fivem": "latest",
    "node-fetch": "^3.3.2"
